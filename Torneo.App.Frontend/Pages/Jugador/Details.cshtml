@page
@model Torneo.App.Frontend.Pages.Jugador.DetailsModel
@{
}
<h1>Detalle de Jugador</h1>

<table class="table table-bordered">
    <tr>
        <th scope="col">Numero</th>
        <td>@Model.jugador.Numero</td>
    </tr>
    <tr>
        <th scope="col">Posicion</th>
        <td>@Model.jugador.Posicion.Nombre</td>
    </tr>
    <tr>
        <th scope="col">Equipo</th>
        <td>@Model.jugador.Equipo.Nombre</td>
    </tr>
    </tbody>
</table>
<a class="btn btn-warning" asp-page="/Jugador/Index">Volver</a>